{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*.php",
      "use": "@vercel/php"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*).php",
      "dest": "/public/$1.php"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_VERSION": "18"
  }
}