{
    "version": 2,
    "routes": [
      {
        "src": "/script.js",
        "dest": "/public/script.js"
      },
      {
        "src": "/style.css",
        "dest": "/public/style.css"
      },
      {
        "src": "/images/(.*)",
        "dest": "/public/images/$1"
      },
      {
        "src": "/api/(.*).php",
        "dest": "/api/$1.php"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
  }